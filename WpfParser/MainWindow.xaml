        <TextBox Name="text_box" Background="Bisque" Width="400" Height="25"></TextBox>
        <Button Click="Button_Click" Margin="0 70 0 0" Height="25" Width="100" Content="Add" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Button>
        <Label Name="label_box" Content="0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Center" Margin="0,295,0,0" VerticalAlignment="Top" Height="33" Width="100"/>


    </Grid>
</Window>
